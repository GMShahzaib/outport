{
  "name": "outport",
  "version": "1.1.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "copy-files": "copyfiles -u 1 \"src/public/**/*.{html,png,css}\" dist/",
    "create-cjs-file": "npx rollup dist/index.js --file dist/index.cjs --format cjs --external express,path,url",
    "build": "tsc && npm run create-cjs-file && npm run copy-files",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "outport",
    "api",
    "apis",
    "connect",
    "express"
  ],
  "author": "Mirza Shahzaib <gms.shahzaib@gmail.com> (https://github.com/GMShahzaib)",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.1.0",
    "copyfiles": "^2.4.1",
    "express": "^4.19.2",
    "rollup": "^4.27.2",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "express": ">=4.0.0 || >=5.0.0-beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GMShahzaib/outport.git"
  },
  "bugs": {
    "url": "https://github.com/GMShahzaib/outport/issues"
  },
  "homepage": "https://github.com/GMShahzaib/outport#readme"
}
